{
	"name": "taxes-api",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "ts-node-dev --respawn --clear index.ts",
		"build": "rimraf ./dist && tsc",
		"start": "npm run build && node ./dist/index.js",
		"postinstall": "npm run build",
		"migrate:generate": "npx sequelize-cli migration:generate --name",
		"migrate:up": "npx sequelize-cli db:migrate",
		"migrate:up:to": "npx sequelize-cli db:migrate --to",
		"migrate:undo:recent": "npx sequelize-cli db:migrate:undo",
		"migrate:undo:to": "npx sequelize-cli db:migrate:undo:all --to",
		"migrate:undo": "npx sequelize-cli db:migrate:undo:all",
		"seed:generate": "npx sequelize-cli seed:generate --name",
		"seed:up": "npx sequelize-cli db:seed:all",
		"seed:up:to": "npx sequelize-cli db:seed --seed",
		"seed:undo:recent": "npx sequelize-cli db:seed:undo",
		"seed:undo:to": "npx sequelize-cli db:seed:undo --seed",
		"seed:undo": "npx sequelize-cli db:seed:undo:all"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"bcrypt": "5.1.1",
		"cors": "2.8.5",
		"express": "4.19.2",
		"ioredis": "5.4.1",
		"jose": "5.3.0",
		"nodemailer": "6.9.13",
		"pg": "8.11.5",
		"pg-hstore": "2.3.4",
		"qs": "6.12.1",
		"sequelize": "6.37.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.2.0",
		"@types/bcrypt": "5.0.2",
		"@types/cors": "2.8.17",
		"@types/express": "4.17.21",
		"@types/node": "20.12.11",
		"@types/nodemailer": "6.4.15",
		"@types/qs": "6.9.15",
		"@types/sequelize": "4.28.20",
		"dotenv": "16.4.5",
		"eslint": "^9.2.0",
		"globals": "^15.2.0",
		"prettier": "3.2.5",
		"rimraf": "5.0.7",
		"sequelize-cli": "6.6.2",
		"ts-node-dev": "2.0.0",
		"typescript": "5.4.5",
		"typescript-eslint": "^7.8.0"
	}
}
